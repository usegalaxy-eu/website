 journal = {International Journal of Molecular Sciences},
 keywords = {\textit{Blattabacterium}, \textit{Blattella germanica}, {\textgreater}UseGalaxy.eu, antimicrobial peptides, cuticle, fat body, metabolite transporters, peptidoglycan-recognition proteins, transcriptome, tyrosine metabolism, uricolytic pathway},
 language = {en},
 month = {January},
