 doi = {10.1093/bfgp/elx033},
 journal = {Briefings in Functional Genomics},
 keywords = {+Workbench, {\textgreater}UseGalaxy.eu},
 month = {November},
 title = {The utility and perspectives of {NGS}-based methods in {BSL}-3 and {BSL}-4 laboratory – sequencing and analysis strategies},
 url = {https://academic.oup.com/bfg/advance-article/doi/10.1093/bfgp/elx033/4616141},
