 doi = {10.1145/3491418.3535177},
 isbn = {978-1-4503-9161-0},
 keywords = {{\textgreater}UseGalaxy.eu},
 month = {July},
 pages = {1--4},
 publisher = {Association for Computing Machinery},
 series = {{PEARC} '22},
 shorttitle = {Custos {Secrets}},
 title = {Custos {Secrets}: a {Service} for {Managing} {User}-{Provided} {Resource} {Credential} {Secrets} for {Science} {Gateways}},
 url = {https://dl.acm.org/doi/10.1145/3491418.3535177},
 urldate = {2024-11-17},
