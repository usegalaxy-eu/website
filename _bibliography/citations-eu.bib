 language = {en},
 month = {April},
 note = {Number: 4
Publisher: Multidisciplinary Digital Publishing Institute},
 number = {4},
 pages = {304},
 title = {Antimicrobial {Activity} of {Bacillus} amyloliquefaciens {BS4} against {Gram}-{Negative} {Pathogenic} {Bacteria}},
