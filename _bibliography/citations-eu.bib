 language = {en},
 month = {March},
 title = {Nucleophosmin 1 cooperates with the methyltransferase {DOT1L} to regulate {H3K79me2} levels and {DNA} satellites expression at peri-nucleolar heterochromatin},
 type = {preprint},
 url = {https://www.researchsquare.com/article/rs-2745386/v1},
 urldate = {2023-04-01},
 year = {2023}
}

