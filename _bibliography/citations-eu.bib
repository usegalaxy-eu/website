 shorttitle = {Exploring {Scutellaria} baicalensis bioactives as {EGFR} tyrosine kinase inhibitors},
 title = {Exploring {Scutellaria} baicalensis bioactives as {EGFR} tyrosine kinase inhibitors: {Cheminformatics} and molecular docking studies},
 url = {https://www.sciencedirect.com/science/article/pii/S2352914823002526},
 urldate = {2023-11-22},
