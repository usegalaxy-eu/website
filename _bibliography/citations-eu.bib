Section: Acta Microbiologica et Immunologica Hungarica},
 number = {aop},
 title = {Genotypic and phenotypic insights into virulence factors of nosocomial {Stenotrophomonas} maltophilia isolates collected in {Bulgaria} (2011–2022)},
 url = {https://akjournals.com/view/journals/030/aop/article-10.1556-030.2023.02059/article-10.1556-030.2023.02059.xml},
