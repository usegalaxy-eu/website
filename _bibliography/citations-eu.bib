 keywords = {{\textgreater}UseGalaxy.eu, Co-expression analysis, Particulate matter, Photinia x fraseri, RNA-seq},
 language = {en},
 month = {May},
 pages = {105313},
 title = {How does particulate matter affect plant transcriptome and microbiome?},
 url = {https://www.sciencedirect.com/science/article/pii/S0098847223001089},
 urldate = {2023-06-05},
 volume = {209},
 year = {2023}
}

