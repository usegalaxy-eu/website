 shorttitle = {Catching the {Wave}},
 title = {Catching the {Wave}: {Detecting} {Strain}-{Specific} {SARS}-{CoV}-2 {Peptides} in {Clinical} {Samples} {Collected} during {Infection} {Waves} from {Diverse} {Geographical} {Locations}},
 volume = {14},
