 journal = {Pathogens},
 keywords = {\textit{Fusarium} species, \textit{Glycine max} (L.) Merrill, {\textgreater}UseGalaxy.eu, RNA polymerase second largest subunit, pathogenicity, root rot, soil-borne fungi, soybean, translation elongation factor 1-alpha},
 language = {en},
 month = {September},
 note = {Number: 9
Publisher: Multidisciplinary Digital Publishing Institute},
 number = {9},
 pages = {1162},
 title = {Identification and {Pathogenicity} of {Fusarium} {Isolated} from {Soybean} in {Poland}},
 url = {https://www.mdpi.com/2076-0817/12/9/1162},
