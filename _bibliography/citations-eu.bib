 title = {Genomic {Evaluation} of {Multidrug}-{Resistant} {Extended}-{Spectrum} β-{Lactamase} ({ESBL})-{Producing} {Escherichia} coli from {Irrigation} {Water} and {Fresh} {Produce} in {South} {Africa}: {A} {Cross}-{Sectional} {Analysis}},
