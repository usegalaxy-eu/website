 issn = {1664-302X},
 journal = {Frontiers in Microbiology},
 keywords = {{\textgreater}UseGalaxy.eu, Antimicrobial resistance (AMR), Delftia tsuruhatensis, Genomics, phylogenomic analysis, raw milk},
 language = {English},
