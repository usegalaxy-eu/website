 pages = {005553},
 title = {Fructobacillus cardui sp. nov., isolated from a thistle ({Carduus} nutans) flower},
 url = {https://www.microbiologyresearch.org/content/journal/ijsem/10.1099/ijsem.0.005553},
 urldate = {2022-11-06},
