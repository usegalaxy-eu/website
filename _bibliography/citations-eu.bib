 language = {en},
 note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/nph.20186},
 number = {n/a},
 shorttitle = {{StCDF1}},
 title = {{StCDF1}: {A} ‘jack of all trades’ clock output with a central role in regulating potato nitrate reduction activity},
