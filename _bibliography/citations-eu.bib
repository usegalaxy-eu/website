 title = {Growth, {Enzymatic}, and {Transcriptomic} {Analysis} of xyr1 {Deletion} {Reveals} a {Major} {Regulator} of {Plant} {Biomass}-{Degrading} {Enzymes} in {Trichoderma} harzianum},
