ISBN: 9781903881606
Journal Abbreviation: Algorithmen zur globalen Abbildung von RNA-RNA Interaktomen},
 title = {Algorithms for the global mapping of {RNA}-{RNA} interactomes},
 type = {{doctoralThesis}},
 url = {http://elib.uni-stuttgart.de/handle/11682/15009},
 urldate = {2024-11-17},
