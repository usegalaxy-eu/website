 shorttitle = {Complete {Mitochondrial} {Genomes} of {Nedyopus} patrioticus},
 title = {Complete {Mitochondrial} {Genomes} of {Nedyopus} patrioticus: {New} {Insights} into the {Color} {Polymorphism} of {Millipedes}},
