 number = {10},
 pages = {e70019},
 title = {Identification of {Pathogenic} {Copy} {Number} {Variants} in {Mexican} {Patients} {With} {Inherited} {Retinal} {Dystrophies} {Applying} an {Exome} {Sequencing} {Data}-{Based} {Read}-{Depth} {Approach}},
