 keywords = {{\textgreater}UseGalaxy.eu, Cell biology, Genetics, Molecular biology, Molecular medicine},
 language = {en},
 month = {July},
 note = {Number: 1
Publisher: Nature Publishing Group},
 number = {1},
 pages = {11477},
 title = {Characterization of regulatory transcriptional mechanisms in hepatocyte lipotoxicity},
 url = {https://www.nature.com/articles/s41598-022-15731-4},
 urldate = {2023-08-06},
 volume = {12},
 year = {2022}
