 language = {en},
 month = {August},
 number = {16},
 pages = {4284--4298.e27},
 title = {A prion-like protein regulator of seed germination undergoes hydration-dependent phase separation},
 url = {https://www.sciencedirect.com/science/article/pii/S0092867421007108},
 urldate = {2021-08-24},
 volume = {184},
