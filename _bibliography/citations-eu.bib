 issn = {2076-0817},
 journal = {Pathogens},
 keywords = {{\textgreater}UseGalaxy.eu, CAZ-AVI, Illumina, MEM-VAR, MLST, cefiderocol, fosfomycin, polymyxin, whole genome sequencing},
 language = {en},
 month = {July},
 note = {Number: 7
Publisher: Multidisciplinary Digital Publishing Institute},
 number = {7},
