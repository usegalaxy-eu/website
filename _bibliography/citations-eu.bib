 number = {1994},
 pages = {20222086},
 title = {Sex-biased gene expression in nutrient-sensing pathways},
 url = {https://royalsocietypublishing.org/doi/full/10.1098/rspb.2022.2086},
 urldate = {2023-12-03},
 volume = {290},
 year = {2023}
}

