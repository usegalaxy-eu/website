 shorttitle = {Rise and fall of {SARS}-{CoV}-2 variants in {Rotterdam}},
 title = {Rise and fall of {SARS}-{CoV}-2 variants in {Rotterdam}: {Comparison} of wastewater and clinical surveillance},
 url = {https://www.sciencedirect.com/science/article/pii/S0048969723008252},
 urldate = {2023-10-28},
