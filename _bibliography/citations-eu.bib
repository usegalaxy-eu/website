 language = {English},
 month = {May},
 note = {Publisher: Frontiers},
 title = {Apoplastomes of contrasting cacao genotypes to witches’ broom disease reveals differential accumulation of {PR} proteins},
 url = {https://www.frontiersin.org/journals/plant-science/articles/10.3389/fpls.2024.1387153/full},
 urldate = {2024-11-17},
 volume = {15},
 year = {2024}
}

