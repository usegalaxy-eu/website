 issn = {1432-0614},
 journal = {Applied Microbiology and Biotechnology},
 keywords = {{\textgreater}UseGalaxy.eu, Directed evolution, Lambda phage, Lambda-evo phage, Phage display, Recombinant-protein, ZIKV, λevo},
 language = {en},
 month = {January},
 number = {1},
 pages = {8},
 title = {A {Lambda}-evo (λevo) phage platform for {Zika} virus {EDIII} protein display},
