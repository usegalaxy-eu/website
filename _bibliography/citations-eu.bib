 isbn = {978-94-6463-164-7},
 keywords = {{\textgreater}UseGalaxy.eu, Science / Life Sciences / Anatomy \& Physiology, Science / Life Sciences / Biology, Science / Life Sciences / Molecular Biology},
 language = {en},
 month = {June},
 note = {Google-Books-ID: GsPDEAAAQBAJ},
 publisher = {Springer Nature},
 title = {Proceedings of the {Joint} 3rd {International} {Conference} on {Bioinformatics} and {Data} {Science} ({ICBDS} 2022)},
