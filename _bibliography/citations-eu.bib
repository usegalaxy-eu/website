 month = {January},
 number = {1},
 pages = {258--273},
 title = {Cuscuta campestris fine-tunes gene expression during haustoriogenesis as an adaptation to different hosts},
 url = {https://doi.org/10.1093/plphys/kiad505},
 urldate = {2024-05-17},
