 language = {en},
 month = {August},
 pages = {144787},
 title = {{SNP} dependent modulation of circulating {miRNAs} from the {miR25}/93/106 cluster in patients undergoing weight loss},
 url = {http://www.sciencedirect.com/science/article/pii/S037811192030456X},
 urldate = {2020-05-28},
 volume = {753},
 year = {2020}
}

