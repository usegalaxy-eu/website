 journal = {Food Research International},
 keywords = {{\textgreater}UseGalaxy.eu, Gene expression, Meta-transcriptome, RNA-seq},
 month = {February},
 pages = {115757},
 title = {Analysis of meta-transcriptomics and identification of genes linked to bioactive peptides and vitamins in {Indonesian} \textit{tempe}},
 url = {https://www.sciencedirect.com/science/article/pii/S0963996925000948},
 urldate = {2025-02-16},
