 note = {Number: 5
Publisher: Multidisciplinary Digital Publishing Institute},
 number = {5},
 pages = {2460},
 title = {Changes in the {Expression} of {Proteins} {Associated} with {Neurodegeneration} in the {Brains} of {Mice} after {Infection} with {Influenza} {A} {Virus} with {Wild} {Type} and {Truncated} {NS1}},
 url = {https://www.mdpi.com/1422-0067/25/5/2460},
