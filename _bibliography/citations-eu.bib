 language = {en},
 month = {October},
 number = {10},
 pages = {1895--1908},
 title = {Genomic {Evidence} for {Sensorial} {Adaptations} to a {Nocturnal} {Predatory} {Lifestyle} in {Owls}},
 url = {https://academic.oup.com/gbe/article/12/10/1895/5889951},
