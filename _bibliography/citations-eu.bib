 number = {2},
 pages = {551--556},
 shorttitle = {{MSstatsShiny}},
 title = {{MSstatsShiny}: {A} {GUI} for {Versatile}, {Scalable}, and {Reproducible} {Statistical} {Analyses} of {Quantitative} {Proteomic} {Experiments}},
 url = {https://doi.org/10.1021/acs.jproteome.2c00603},
