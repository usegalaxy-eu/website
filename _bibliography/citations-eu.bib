 journal = {Scientific Reports},
 keywords = {{\textgreater}UseGalaxy.eu, Infectious diseases, SARS-CoV-2, Viral epidemiology, Virology},
 language = {en},
 month = {December},
 note = {Number: 1
Publisher: Nature Publishing Group},
 number = {1},
 pages = {22414},
 title = {Genomic epidemiology of {SARS}- {CoV}-2 {Omicron} variants in the {Republic} of {Korea}},
