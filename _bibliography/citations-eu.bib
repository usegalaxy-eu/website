 copyright = {Copyright © 2023 the authors. SfN exclusive license.},
 doi = {10.1523/JNEUROSCI.1824-22.2023},
 issn = {0270-6474, 1529-2401},
 journal = {Journal of Neuroscience},
 keywords = {{\textgreater}UseGalaxy.eu, amyloid precursor protein, dentate gyrus, entorhinal cortex, hilar mossy cell, perforant path, stellate cells},
 language = {en},
 month = {July},
 note = {Publisher: Society for Neuroscience
Section: Research Articles},
 number = {29},
