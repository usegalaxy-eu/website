 month = {February},
 pages = {109919},
 title = {Transcriptome dataset of gall-rust infected {Sengon} (\textit{{Falcataria} falcata}) seedlings using long-read {PCR}-{cDNA} sequencing},
 url = {https://www.sciencedirect.com/science/article/pii/S2352340923009587},
 urldate = {2024-05-17},
 volume = {52},
