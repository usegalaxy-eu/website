 month = {July},
 number = {8},
 pages = {247},
 shorttitle = {Unveiling {Agricultural} {Biotechnological} {Prospects}},
 title = {Unveiling {Agricultural} {Biotechnological} {Prospects}: {The} {Draft} {Genome} {Sequence} of {Stenotrophomonas} geniculata {LGMB417}},
 url = {https://doi.org/10.1007/s00284-024-03784-9},
 urldate = {2024-07-05},
