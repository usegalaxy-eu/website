 number = {5},
 pages = {e00046--23},
 title = {Species {Delineation} and {Comparative} {Genomics} within the {Campylobacter} ureolyticus {Complex}},
 url = {https://journals.asm.org/doi/full/10.1128/jcm.00046-23},
 urldate = {2023-06-05},
