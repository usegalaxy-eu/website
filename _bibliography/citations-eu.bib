Publisher: Multidisciplinary Digital Publishing Institute},
 number = {2},
 pages = {113},
 title = {The {Genome} of {Arsenophonus} sp. and {Its} {Potential} {Contribution} in the {Corn} {Planthopper}, {Peregrinus} maidis},
