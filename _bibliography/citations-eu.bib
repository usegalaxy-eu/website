 language = {English},
 month = {March},
 note = {Publisher: Frontiers},
 title = {The altered {TBI} fecal microbiome is stable and functionally distinct},
 url = {https://www.frontiersin.org/articles/10.3389/fnmol.2024.1341808},
 urldate = {2024-05-17},
 volume = {17},
 year = {2024}
