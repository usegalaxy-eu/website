 journal = {Nature Communications},
 keywords = {{\textgreater}UseGalaxy.eu, Acetylation, Adipocytes, Adipose Tissue, Amino Acids, Animals, Carbon, Diabetes Mellitus, Type 2, Diet, High-Fat, Gene Expression Regulation, Genetic Predisposition to Disease, Glucose, Glucose Transporter Type 4, Haploinsufficiency, Histone Acetyltransferases, Histones, Lipid Metabolism, Lysine, Mice, Obesity, PPAR gamma},
 language = {eng},
 month = {October},
 number = {1},
 pages = {6212},
 pmcid = {PMC8551339},
 pmid = {34707105},
 title = {Histone {H4} lysine 16 acetylation controls central carbon metabolism and diet-induced obesity in mice},
