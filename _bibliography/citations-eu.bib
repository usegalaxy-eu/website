 journal = {International Journal of Molecular Sciences},
 keywords = {\textit{Prunus salicina}, \textit{S}-allele, \textit{S}-genotyping-by-sequencing, {\textgreater}UseGalaxy.eu, Japanese plum, self-incompatibility},
 language = {en},
 month = {January},
 note = {Number: 4
Publisher: Multidisciplinary Digital Publishing Institute},
 number = {4},
 pages = {3932},
 title = {S-{Locus} {Genotyping} in {Japanese} {Plum} by {High} {Throughput} {Sequencing} {Using} a {Synthetic} {S}-{Loci} {Reference} {Sequence}},
 url = {https://www.mdpi.com/1422-0067/24/4/3932},
