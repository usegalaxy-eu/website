 title = {The {Rhodoexplorer} {Platform} for {Red} {Algal} {Genomics} and {Whole}-{Genome} {Assemblies} for {Several} {Gracilaria} {Species}},
 url = {https://doi.org/10.1093/gbe/evad124},
 urldate = {2024-11-17},
