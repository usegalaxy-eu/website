 issn = {0191-2917},
 journal = {Plant Disease},
 keywords = {{\textgreater}UseGalaxy.eu},
 month = {January},
 note = {Publisher: Scientific Societies},
 title = {Pathovar-{Specific} {PCR} {Method} for {Detection} and {Identification} of {Xanthomonas} translucens pv. undulosa},
