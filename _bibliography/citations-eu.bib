 issn = {0261-4189},
 journal = {The EMBO Journal},
 keywords = {+Methods, +UsePublic, {\textgreater}UseGalaxy.eu, AP-1, ZEB1, breast cancer, epithelial to mesenchymal transition},
 month = {July},
 note = {Publisher: John Wiley \& Sons, Ltd},
 number = {n/a},
