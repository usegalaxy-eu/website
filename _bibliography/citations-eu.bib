 month = {May},
 note = {Publisher: IOP Publishing},
 number = {1},
 pages = {012023},
 title = {Bacterial key species candidates for biomonitoring peatland burnt in the {Giam} {Siak} {Kecil}-{Bukit} {Batu} biosphere reserve, {Riau}},
 url = {https://doi.org/10.1088/1755-1315/1025/1/012023},
 urldate = {2022-09-24},
