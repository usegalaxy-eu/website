 issn = {1096-9071},
 journal = {Journal of Medical Virology},
 keywords = {+Methods, +UsePublic, {\textgreater}UseGalaxy.eu, ACE2, COVID-19, SARS-CoV-2, TMPRSS2, human conjunctiva},
 language = {en},
