 keywords = {{\textgreater}ChemicalToolbox, Bismarck Brown Y, C.I. 21000, Circular Dichroism Spectroscopy, Fluorescence Spectroscopy, Molecular Docking, Molecular Dynamics, chemicaltoolbox},
 month = {October},
 pages = {107976},
 title = {Structure and {Energetics} of {Serum} {Protein} {Complex} of {Tea} {Adulterant} {Dye} {Bismarck} {Brown} {Y} using {Experimental} and {Computational} {Methods}},
 url = {https://www.sciencedirect.com/science/article/pii/S1476927123001676},
 urldate = {2023-11-04},
 year = {2023}
}

