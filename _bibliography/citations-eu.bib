 doi = {10.1038/s41418-022-00992-3},
 issn = {1476-5403},
 journal = {Cell Death \& Differentiation},
 keywords = {{\textgreater}UseGalaxy.eu, Epigenetics, Inflammasome},
 language = {en},
 month = {October},
 note = {Number: 10
Publisher: Nature Publishing Group},
