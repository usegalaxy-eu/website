 language = {en},
 month = {May},
 note = {Number: 1
Publisher: Nature Publishing Group},
 number = {1},
 pages = {7287},
 shorttitle = {Mechanism-based and computational modeling of hydrogen sulfide biogenesis inhibition},
 title = {Mechanism-based and computational modeling of hydrogen sulfide biogenesis inhibition: interfacial inhibition},
 url = {https://www.nature.com/articles/s41598-023-34405-3},
 urldate = {2023-07-31},
