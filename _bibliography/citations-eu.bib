 title = {Δ{N}-{Tp63} {Mediates} {Wnt}/β-{Catenin}-{Induced} {Inhibition} of {Differentiation} in {Basal} {Stem} {Cells} of {Mucociliary} {Epithelia}},
 url = {http://www.sciencedirect.com/science/article/pii/S2211124719311180},
