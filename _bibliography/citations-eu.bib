 keywords = {{\textgreater}UseGalaxy.eu},
 language = {en},
 month = {November},
 note = {Number: 11},
 number = {11},
 title = {Comparative study of chloroplast genomes across seven {Salacca} species},
 url = {https://smujo.id/biodiv/article/view/18286},
