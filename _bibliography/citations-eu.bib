 keywords = {{\textgreater}UseGalaxy.eu, Agricultural management practices, Agroecosystems, Bacteria, Fungi, Soil microbial functional groups},
 month = {December},
 pages = {105723},
 title = {Land use intensity differently affects soil microbial functional communities in arable fields},
 url = {https://www.sciencedirect.com/science/article/pii/S0929139324004542},
