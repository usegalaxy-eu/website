 language = {en},
 note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/ece3.11696},
 number = {7},
 pages = {e11696},
 shorttitle = {A reproducible workflow for assembling the mitochondrial genome of {Acheta} domesticus ({Orthoptera}},
 title = {A reproducible workflow for assembling the mitochondrial genome of {Acheta} domesticus ({Orthoptera}: {Gryllidae})},
 url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/ece3.11696},
