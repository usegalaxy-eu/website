 keywords = {+Methods, +UsePublic, {\textgreater}UseGalaxy.eu},
 month = {October},
 title = {Highly efficient library preparation for {Ion} {Torrent} sequencing using {Y}-adapters},
 url = {https://www.future-science.com/doi/10.2144/btn-2019-0035},
