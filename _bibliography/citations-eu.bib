 pages = {108545},
 title = {{CDK8} {Fine}-{Tunes} {IL}-6 {Transcriptional} {Activities} by {Limiting} {STAT3} {Resident} {Time} at the {Gene} {Loci}},
 url = {https://www.sciencedirect.com/science/article/pii/S2211124720315345},
 urldate = {2021-07-21},
 volume = {33},
 year = {2020}
