 language = {en},
 month = {July},
 number = {7},
 pages = {766--779.e11},
 title = {Peptide-mediated inhibition of the transcriptional regulator {Elongin} {BC} induces apoptosis in cancer cells},
 url = {https://www.sciencedirect.com/science/article/pii/S2451945623001551},
 urldate = {2023-07-31},
