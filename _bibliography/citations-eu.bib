 pmcid = {PMC10315552},
 pmid = {37183016},
 title = {Detection of domestic cat hepadnavirus by next-generation sequencing and epidemiological survey in {Japan}},
 url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC10315552/},
 urldate = {2023-10-07},
