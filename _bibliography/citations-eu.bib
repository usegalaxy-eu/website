 language = {en},
 month = {October},
 note = {Publisher: Nature Publishing Group},
 number = {1},
 pages = {9171},
 title = {Epigenetic modulation via the {C}-terminal tail of {H2A}.{Z}},
 url = {https://www.nature.com/articles/s41467-024-53514-9},
