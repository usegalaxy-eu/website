 journal = {Physiological and Molecular Plant Pathology},
 keywords = {{\textgreater}UseGalaxy.eu, Cell death, Molecular docking, NEP2 effector},
 language = {en},
 month = {March},
 pages = {101946},
 title = {Proteomics analysis reveals three potential cacao target that interacts with {Moniliophthora} perniciosa {NEP} during witches broom disease},
 url = {https://www.sciencedirect.com/science/article/pii/S0885576523000012},
