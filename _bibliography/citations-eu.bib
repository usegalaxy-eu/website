 publisher = {Social Science Research Network},
 title = {Fungani, a {Blast}-{Based} {Program} for {Analyzing} {Average} {Nucleotide} {Identity} ({Ani}) between {Two} {Fungal} {Genomes}, {Enables} {Easy} {Fungal} {Species} {Delimitation}},
 type = {{SSRN} {Scholarly} {Paper}},
