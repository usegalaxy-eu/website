 number = {2},
 pages = {93--103},
 shorttitle = {More {May} {Not} be {Better}},
 title = {More {May} {Not} be {Better}: {Enhanced} {Spacecraft} {Shielding} {May} {Exacerbate} {Cognitive} {Decrements} by {Increasing} {Pion} {Exposures} during {Deep} {Space} {Exploration}},
 url = {https://doi.org/10.1667/RADE-23-00241.1.S1},
