 doi = {10.1002/art.42781},
 issn = {2326-5191},
 journal = {Arthritis and Rheumatology},
 keywords = {{\textgreater}UseGalaxy.eu},
 month = {May},
 number = {5},
 pages = {783--795},
 title = {Effect of {Anti}-{S100A4} {Monoclonal} {Antibody} {Treatment} on {Experimental} {Skin} {Fibrosis} and {Systemic} {Sclerosis}–{Specific} {Transcriptional} {Signatures} in {Human} {Skin}},
