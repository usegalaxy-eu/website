 title = {Sediment {Plasmidome} of the {Gulfs} of {Kathiawar} {Peninsula} and {Arabian} {Sea}: {Insights} {Gained} from {Metagenomics} {Data}},
 url = {https://doi.org/10.1007/s00248-020-01587-6},
 urldate = {2021-05-12},
