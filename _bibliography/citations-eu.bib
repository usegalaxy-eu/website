 title = {Precursor {Intensity}-{Based} {Label}-{Free} {Quantification} {Software} {Tools} for {Proteomic} and {Multi}-{Omic} {Analysis} within the {Galaxy} {Platform}},
 url = {https://www.mdpi.com/2227-7382/8/3/15},
