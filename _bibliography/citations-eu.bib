 language = {en},
 month = {October},
 note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/tbed.13878},
 number = {n/a},
 title = {Next generation sequencing study on {RNA} viruses of {Vespa} velutina and {Apis} mellifera sharing the same foraging area},
