 title = {Genetic variability of {New} {Caledonian} {Boeckella} {De} {Guerne} \& {Richard}, 1889 ({Copepoda}: {Calanoida}), with the description of a new species},
 url = {https://doi.org/10.1093/jcbiol/ruae001},
